{
  "name": "@mappedin/mappedin-js",
  "version": "5.39.3",
  "main": "lib/esm/renderer/index.js",
  "module": "lib/esm/renderer/index.js",
  "browser": "lib/esm/renderer/index.js",
  "types": "lib/esm/renderer/index.d.ts",
  "files": [
    "lib",
    "README.md"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MappedIn/mappedin-core-sdk.git"
  },
  "scripts": {
    "rel:patch": "node scripts/release.mjs patch",
    "start": "yarn build && concurrently \"node scripts/build.mjs --watchChanges\" \"node scripts/start.mjs\" \"yarn copy-styles\" ",
    "start:fast": "yarn build && concurrently \"node scripts/build.mjs --watchChanges\" \"node scripts/start.mjs --fast\"",
    "build": "yarn clean && tsc -b && copyfiles ../packages/legacy-renderer/**/*.css && cd ../packages/legacy-renderer && copyfiles **/*.scss **/*.png **/*.glsl **/*.svg ../.cache/legacy-renderer && cd ../../cms && node scripts/build.mjs",
    "build:watch": "concurrently \"node scripts/build.mjs --watchChanges\" \"yarn types --watch\"",
    "build:prod": "cross-env NODE_ENV=production yarn build && node scripts/minify.mjs",
    "types": "tsc -b .",
    "typecheck": "tsc --noEmit",
    "copy-styles": "cp lib/esm/renderer/index.css lib/esm/index.css",
    "clean": "rm -rf lib/**",
    "docs": "yarn build && typedoc"
  },
  "devDependencies": {
    "@types/offscreencanvas": "^2019.6.3",
    "archiver": "^5.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-three": "^1.3.1"
  },
  "gitHead": "b86ff42103683c0268ac74beecca011ef9c63611",
  "volta": {
    "extends": "../package.json"
  }
}
